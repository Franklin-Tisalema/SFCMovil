<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:showIn="@layout/activity_consulta_totales_pagos"
    tools:context="com.chibuleo.sfcmovil.ConsultaTotalesPagos"
    android:padding="0dp">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Usuario:"
        android:id="@+id/lblTituloUsuario"
        android:layout_x="155dp"
        android:layout_y="5dp"
        android:textColor="#000000"
        android:textStyle="bold" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/lblUsuario"
        android:layout_x="210dp"
        android:layout_y="5dp"
        android:textColor="@color/colorEtiquetaTitulo"
        android:textStyle="bold" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/lblFecha"
        android:layout_x="210dp"
        android:layout_y="25dp"
        android:textColor="@color/colorEtiquetaTitulo"
        android:textStyle="bold" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Fecha:"
        android:id="@+id/lblTituloFecha"
        android:layout_x="155dp"
        android:layout_y="25dp"
        android:textColor="#000000"
        android:textStyle="bold" />

    <View android:id="@+id/primerdivisor"
        android:layout_height="2dp"
        android:layout_width="fill_parent"
        android:background="#000000"
        android:layout_x="0dp"
        android:layout_y="45dp"/>

    <ImageView
        android:layout_width="145dp"
        android:layout_height="45dp"
        android:id="@+id/imageView"
        android:layout_x="0dp"
        android:layout_y="0dp"
        android:src="@mipmap/logoch" />

    <TextView
        android:id="@+id/lblPagosInLine"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_x="0dp"
        android:layout_y="50dp"
        android:gravity="center_vertical|center_horizontal"
        android:text="TRANSACCIONES EN LÍNEA"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Numero:"
        android:id="@+id/lblNumeroI"
        android:layout_x="5dp"
        android:layout_y="80dp"
        android:textStyle="bold"
        android:textColor="#000000" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Valor:"
        android:id="@+id/lblValorI"
        android:layout_x="5dp"
        android:layout_y="105dp"
        android:textStyle="bold"
        android:textColor="#000000" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Numero"
        android:id="@+id/txtNumeroI"
        android:layout_x="90dp"
        android:layout_y="80dp"
        android:textStyle="bold"
        android:textColor="#9d0404" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Valor"
        android:id="@+id/txtValorI"
        android:layout_x="90dp"
        android:layout_y="105dp"
        android:textStyle="bold"
        android:textColor="#9d0404" />

    <View android:id="@+id/segundodivisor"
        android:layout_height="1dp"
        android:layout_width="fill_parent"
        android:background="#000000"
        android:layout_x="0dp"
        android:layout_y="130dp"/>

    <TextView
        android:id="@+id/lblPagosOffLine"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_x="0dp"
        android:layout_y="140dp"
        android:gravity="center_vertical|center_horizontal"
        android:text="TRANSACCIONES FUERA DE LÍNEA"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Numero:"
        android:id="@+id/lblNumeroO"
        android:layout_x="5dp"
        android:layout_y="172dp"
        android:textStyle="bold"
        android:textColor="#000000" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Numero"
        android:id="@+id/txtNumeroO"
        android:layout_x="90dp"
        android:layout_y="172dp"
        android:textStyle="bold"
        android:textColor="#9d0404" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Valor:"
        android:id="@+id/lblValorO"
        android:layout_x="5dp"
        android:layout_y="196dp"
        android:textStyle="bold"
        android:textColor="#000000" />

    <View android:id="@+id/tercerdivisor"
        android:layout_height="1dp"
        android:layout_width="fill_parent"
        android:background="#000000"
        android:layout_x="0dp"
        android:layout_y="230dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Valor"
        android:id="@+id/txtValorO"
        android:layout_x="90dp"
        android:layout_y="196dp"
        android:textStyle="bold"
        android:textColor="#9d0404" />

    <TextView
        android:id="@+id/lblPagosTotales"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_x="0dp"
        android:layout_y="238dp"
        android:gravity="center_vertical|center_horizontal"
        android:text="TRANSACCIONES TOTALES"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Numero:"
        android:id="@+id/lblNumero"
        android:layout_x="5dp"
        android:layout_y="270dp"
        android:textStyle="bold"
        android:textColor="#000000" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Numero"
        android:id="@+id/txtNumero"
        android:layout_x="90dp"
        android:layout_y="270dp"
        android:textStyle="bold"
        android:textColor="#9d0404" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Valor:"
        android:id="@+id/lblValor"
        android:layout_x="5dp"
        android:layout_y="295dp"
        android:textStyle="bold"
        android:textColor="#000000" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Valor"
        android:id="@+id/txtValor"
        android:layout_x="90dp"
        android:layout_y="295dp"
        android:textStyle="bold"
        android:textColor="#9d0404" />

    <Button
        android:id="@+id/btnImprimeCobrosTotales"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_x="0dp"
        android:layout_y="337dp"
        android:background="@drawable/borde_fondo_naranja"
        android:enabled="true"
        android:focusable="true"
        android:text="Imprime Totales Recaudados"
        android:textStyle="bold" />


</AbsoluteLayout>
